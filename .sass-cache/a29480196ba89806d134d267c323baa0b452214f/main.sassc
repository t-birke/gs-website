@import susy

// Variables
$main-color: #ff00aa
$shadow-color: #880044
$text-color: #3b3b3b
$gs-yellow: #ffcc00
$bg-grey: #3b3b3b


// susy setup
$total-columns  : 12             // a 12-column grid
$column-width   : 60px            // each column is 4em wide
$gutter-width   : 20px           // 1em gutters between columns
$grid-padding   : 10px  // grid-padding equal to gutters
$container-style: magic


.page
  // Establish our default 4-column grid container.
  @include container
  background-color: $main-color

@include at-breakpoint(480px 12)
  .page
    @include container

.left-box
  background-color: $shadow-color
  @include span-columns(6)

//Font definition for heading fonts

+font-face("Novecentosanswide", font-files("Novecentosanswide-Bold-webfont.woff", woff, "Novecentosanswide-Bold-webfont.ttf", truetype, "Novecentosanswide-Bold-webfont.svg", svg), "Novecentosanswide-Bold-webfont.eot", $weight: 800, $style: bold)
+font-face("Novecentosanswide", font-files("Novecentosanswide-DemiBold-webfont.woff", woff, "Novecentosanswide-DemiBold-webfont.ttf", truetype, "Novecentosanswide-DemiBold-webfont.svg", svg), "Novecentosanswide-DemiBold-webfont.eot", $weight: 600, $style: normal)
+font-face("Novecentosanswide", font-files("Novecentosanswide-Medium-webfont.woff", woff, "Novecentosanswide-Medium-webfont.ttf", truetype, "Novecentosanswide-Medium-webfont.svg", svg), "Novecentosanswide-Medium-webfont.eot", $weight: 400, $style: thin)
+font-face("IconFont", font-files("gs-icon-font.woff", woff, "gs-icon-font.ttf", truetype, "gs-icon-font.svg#gs-icon-font", svg), "gs-icon-font.eot", $weight: normal, $style: normal)

html
  height: 100%

body
  min-width: 960px
  height: 100%
  margin: 0
  font: 16px/24px "Open Sans", "Helvetica Neue", sans-serif
  background: white
  -webkit-font-smoothing: antialiased

a,
a:visited, 
a:link, 
a:hover, 
a:active
  color: $text-color
  outline: none
  text-decoration: none

.ico
  font-family: 'IconFont'
  font-weight: normal
  font-style: normal
  -webkit-font-smoothing: antialiased

input,
input:focus
  +box-shadow(none)
  outline: none
//menu styles

h1,
h2,
h3
