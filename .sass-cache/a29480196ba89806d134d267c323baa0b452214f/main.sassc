@import susy

// Variables
$main-color: #ff00aa
$shadow-color: #880044
$text-color: #3b3b3b
$gs-yellow: #ffcc00
$bg-grey: #3b3b3b


// susy setup
$total-columns  : 12             // a 12-column grid
$column-width   : 60px            // each column is 4em wide
$gutter-width   : 20px           // 1em gutters between columns
$grid-padding   : 10px  // grid-padding equal to gutters
$container-style: magic


.page
  // Establish our default 4-column grid container.
  @include container
  background-color: $main-color

@include at-breakpoint(480px 12)
  .page
    @include container

.left-box
  background-color: $shadow-color
  @include span-columns(6)

//Font definition for heading fonts

+font-face("Novecentosanswide", font-files("Novecentosanswide-Bold-webfont.woff", woff, "Novecentosanswide-Bold-webfont.ttf", truetype, "Novecentosanswide-Bold-webfont.svg", svg), "Novecentosanswide-Bold-webfont.eot", $weight: 800, $style: bold)
+font-face("Novecentosanswide", font-files("Novecentosanswide-DemiBold-webfont.woff", woff, "Novecentosanswide-DemiBold-webfont.ttf", truetype, "Novecentosanswide-DemiBold-webfont.svg", svg), "Novecentosanswide-DemiBold-webfont.eot", $weight: 600, $style: normal)
+font-face("Novecentosanswide", font-files("Novecentosanswide-Medium-webfont.woff", woff, "Novecentosanswide-Medium-webfont.ttf", truetype, "Novecentosanswide-Medium-webfont.svg", svg), "Novecentosanswide-Medium-webfont.eot", $weight: 400, $style: thin)
+font-face("IconFont", font-files("gs-icon-font.woff", woff, "gs-icon-font.ttf", truetype, "gs-icon-font.svg#gs-icon-font", svg), "gs-icon-font.eot", $weight: normal, $style: normal)

html
  height: 100%

body
  min-width: 960px
  height: 100%
  margin: 0
  font: 16px/24px "Open Sans", "Helvetica Neue", sans-serif
  background: white
  -webkit-font-smoothing: antialiased

a,
a:visited, 
a:link, 
a:hover, 
a:active
  color: $text-color
  outline: none
  text-decoration: none

.ico
  font-family: 'IconFont'
  font-weight: normal
  font-style: normal
  -webkit-font-smoothing: antialiased

input,
input:focus
  +box-shadow(none)
  outline: none
//menu styles

h1,
h2,
h3
  text-align: center
  text-transform: uppercase

h1
  font-family: 'Novecentosanswide'
  font-weight: 800

h2
  font-family: 'Novecentosanswide'
  font-weight: 600
  
h3
  font-family: 'Novecentosanswide'
  font-weight: 400

.center
  text-align: center

header
  +box-shadow(0 0 12px rgba(0,0,0,0.1))
  position: fixed
  top: 0
  left: 0
  right: 0
  height: 60px
  background-color: white
  line-height: 60px
  z-index: 100

  .menu
    position: relative
    width: 960px
    text-align: center
    margin: auto

  .logo-hat
    position: absolute
    top: 0
    left: 46.5%
    width: 7%
    height: 100%
    background: url('../images/logo_1.png') no-repeat center center
    background-size: contain

  a
    +border-radius(5px)
    margin: 0 2em
    padding: .5em
    font-family: 'Novecentosanswide'
    font-weight: 400
    font-size: .85em
    border: 1px solid transparent
    text-transform: lowercase

    &:hover
      border-color: $gs-yellow
      cursor: pointer

    &.active
      +transition(all 0.5s ease)
      border-color: $gs-yellow
      background-color: transparent

    &.active-transition
      +transition(all 0.5s ease)
      border-color: $gs-yellow
      background-color: $gs-yellow

    &.inactive
      +transition(all 0.5s ease)
      border-color: transparent
      background-color: transparent

  a:visited, 
  a:link, 
  a:hover, 
  a:active
    color: $text-color


  .push
    margin-right: 11em

h1
  font-size: 2.8em
  color: white

h3
  font-size: 1em
  color: white
  margin-top: 9em

  &.stilcorrection
    margin-top: 5em

.subheading
  position: relative
  font-size: 0.94em
  margin: 4em auto
  width: 26em
  color: white
  text-align: center

  &:after
    content: ""
    position: absolute
    top: -1em
    left: 4em
    width: 18.75em
    height: 1px
