      font-size: 80%
    @media only screen and (max-width: 480px)
      font-size: 60%

    h3
      position: relative
      font-size: 1.875em
      margin: 0
      font-family: "Open Sans"
      padding: 0 20px

      &:after
        content: ""
        position: absolute
        bottom: -20px
        left: 33%
        width: 33%
        height: 1px
        background-color: white

    p
      font-size: .75em
      text-align: center
      color: white
      margin-top: 30px
      padding: 0 20px

    .popup-links
      text-align: center
      margin-top: 30px
      color: #3e3e3e
      padding: 0 20px

    a
      font-family: "Novecentosanswide"
      font-weight: 600
      font-size: .75em
      color: #3e3e3e
      text-transform: uppercase

    .close
      +scaleY(0.7)
      position: absolute
      top: 10px
      right: 14px
      font-size: 20px
      color: white

      &:hover
        cursor: pointer

  img
    width: 100%
    margin: -3px 0
    padding: 0

.text-anchor
  font-size: 0

.service-menu
  height: 44em
  width: 100%
  background-color: white
  text-align: center

  .service-menu-background
    height: 15em
    width: 100%
    background-color: $bg-grey

  .service-menu-container
    position: relative
    width: 960px
    margin: 0 auto

  .service-container
    vertical-align: top
    width: 960px
    margin: auto

  a:visited, 
  a:link, 
  a:hover, 
  a:active
    color: white

  a
    display: inline-block
    position: relative
    width: 6.5em
    margin: 5.5em 5.5em 0 5.5em
    font-family: 'Novecentosanswide'
    font-weight: 400
    font-size: 1em
    color: white
    text-transform: uppercase

    &:hover
