.facebook
  color: #6279b1

.instagram
  color: #ffdb4c

.dribble
  color: #ff3398

.behance
  color: #33c7f1

.section-white-bg
  position: absolute
  top: 450px
  left: 0
  background-color: white
  width: 100%
  height: 100%
  z-index: -1

.hidden
  display: none

#popup-background
  position: fixed
  top: 0
  right: 0
  bottom: 0
  left: 0
  background-color: rgba(222,0,0,0.85)
  z-index: 1000
  overflow-y: scroll

.popup
  +box-shadow(0 0 20px rgba(0,0,0,0.7))
  margin: 40px auto
  width: 90%
  max-width: 880px
  background-color: white
  height: 1400px

  .popup-header
    position: relative
    width: 100%
    background-color: #1b1b1b
    padding: 50px 0
