    &:hover
      border-color: $gs-yellow

    &.active
      +transition(all 0.5s ease)
      border-color: $gs-yellow
      background-color: transparent

    &.active-transition
      +transition(all 0.5s ease)
      border-color: $gs-yellow
      background-color: $gs-yellow

    &.inactive
      +transition(all 0.5s ease)
      border-color: transparent
      background-color: transparent

  a:visited, 
  a:link, 
