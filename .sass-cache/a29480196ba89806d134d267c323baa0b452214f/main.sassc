      font-weight: 600
      font-size: 12px
      color: $bg-grey
      height: 4em
      line-height: 4em
      border: 1px solid #ddd
      border-top: none
      background-color: white

    .first
      border-left: none
      border-right: none
  
      &:after
        content: "\25BC"
        position: absolute
        top: .3em
        right: 7em
        font-size: .6em
        color: $gs-yellow
      
      &:hover
        cursor: pointer

    .mobile
      float: none
      font-size: 1.2em
      margin: 0

      &:hover
        cursor: pointer

    &.show
      height: 22em
      +transition(all 0.5s ease)

  .push
    float: right

