        display: inline-block
        margin: 0 .3em

      .facebook
        &:hover
          color: #6279b1

      .linkedin
        &:hover
          color: #1d87bd

      .pinterest
        &:hover
          color: #cb2027

      .xing
        &:hover
          color: #CEDB00

    .contact-text
      float: left
      width: 29em
      @media only screen and (max-width: 767px)
        width: 100%
        text-align: center

      h2
        font-size: 1.25em
        color: $text-color
        text-align: left
        margin: 0
        @media only screen and (max-width: 767px)
          text-align: center

      .name
        font-size: .94em
        color: $text-color
        text-align: left

      p
        position: relative
        font-size: .75em
        color: $text-color
        line-height: 1.5em
        margin-top: 4em

        &:after
          content: ""
          position: absolute
          top: -2em
          left: 0
          width: 24em
          height: 1px
          background-color: $gs-yellow
          @media only screen and (max-width: 767px)
            width: 18em
            left: 2em

        &:before
          content: ""
          position: absolute
          bottom: -3em
          left: 0
          width: 24em
          height: 1px
          background-color: $gs-yellow
          @media only screen and (max-width: 767px)
            width: 18em
            left: 2em

    .contact-rest
      clear: both
      margin-top: 24em
      text-align: center
      font-size: .7em
      @media only screen and (max-width: 767px)
        margin-top: 30em

      .button
        display: inline-block
        margin-top: 1.5em
        padding: 1em 0 0 0
        height: 30px
        color: $gs-yellow

footer
  width: 100%

  .refer
    float: left
    width: 42%
    height: 300px
    background-color: #2f2f2f
    @media only screen and (max-width: 767px)
      float: none
      width: 100%

    .refer-container
      float: right
      width: 17.5em
      margin: 1em 1em 0 0
      overflow: hidden
      @media only screen and (max-width: 767px)
        float: none
        margin: 1em auto
        text-align: center

      .refer-heading
        position: relative

      .avatar
        position: absolute
        top: 0.1em
        left: 0
        display: block
        float: left
        font-size: 3.6em
        color: $text-color

      h2
        font-size: .8em
        color: white
        text-align: left
        margin: 1.5em 0 0 6em
        line-height: 1.5em

      .refer-call
        font-size: .7em
        color: white
        text-align: left
        margin: 0 0 0 6.7em
        line-height: 1.5em

      .refer-text
        margin: 4em 0 0 0
        color: #999
        line-height: 1.5em
        font-size: .7em
        text-align: left
        @media only screen and (max-width: 767px)
          text-align: center

      .refer-social
        position: relative
        font-size: .8em
        margin-top: 1.5em
        color: white
        text-align: left
        @media only screen and (max-width: 767px)
          text-align: center

        &:before
          content: ""
          position: absolute
          bottom: -.6em
          left: 0
          width: 12.5em
          font-size: 1em
          height: 1px
          background-color: $text-color
          @media only screen and (max-width: 767px)
            left: 4.8em

      .social
        display: inline-block
        margin: 0 .5em
        font-size: 1.2em

        &:first
          margin-left: 3em

      .button
        margin-top: 2em

  .login
    float: left
    width: 58%
    height: 300px
    background-color: #353535
    @media only screen and (max-width: 767px)
      display: none

    h2
      font-size: 1.3em
      color: white
      text-align: left
      margin: 1.5em 0 0 3.1em
      line-height: 1.5em

    .login-subheading
      font-size: .9em
      color: white
      text-align: left
      margin: 0 0 0 4.4em
      line-height: 1.5em

    .description
      margin: 2.6em 0 0 5.7em
      width: 42em
      font-size: .7em
      color: #999
      line-height: 1.5em

    form
      margin: 1.2em 0 0 4em

    input
      width: 200px
      height: 16px
      padding: 10px 0 10px 20px
      line-height: normal !important
      font-size: 12px
      font-family: 'Novecentosanswide'
      font-weight: 600
      color: #ddd
      text-transform: uppercase
      outline: none
      border: 1px solid #444
      background-color: #353535

    .button
      margin: 1em 0 0 0

  .maps
    clear: both
    float: left
    width: 42%
    height: 300px
    @media only screen and (max-width: 767px)
      float: none
      width: 100%
      height: 150px

    #map-canvas
      height: 300px
      @media only screen and (max-width: 767px)
        height: 150px

  .contact
    float: left
    width: 58%
    height: 300px
    background-color: #3b3b3b
    @media only screen and (max-width: 767px)
      float: none
      width: 100%
      margin-top: -31px
      padding-top: 2em

    .contact-container
      width: 16em
      @media only screen and (max-width: 767px)
        margin: auto

    h2
      font-size: 1.3em
      color: white
      text-align: left
      margin: 1.5em 0 0 3.1em
      line-height: 1.5em
      @media only screen and (max-width: 767px)
        font-size: 1em
        margin: 1.5em

    .address
      margin-left: 5.33em
      font-size: .75em
      color: white
      line-height: 1.6em
      @media only screen and (max-width: 767px)
        margin-left: 2em

      a,
      a:visited,
      a:active,
      a:hover,
      a:link
        color: $gs-yellow

    .contact-links
      margin: 4em 0 0 5.33em
      font-size: .75em
      color: white
      @media only screen and (max-width: 767px)
        margin-left: 2em

      a,
      a:visited,
      a:active,
      a:hover,
      a:link
        color: white
        text-decoration: underline

// other sites stiles
.other  
  .content
    width: 960px
    padding: 0 1em
    margin: auto
    overflow: hidden

  .backlink
    clear: both
    float: left
    height: 180px
    padding: 170px 0 0 0
    width: 340px

    a
      color: #b3b190
      font-family: "Novecentosanswide"
      font-weight: 600
      font-size: .75em

  .logo
    float: left
    height: 220px
    width: 246px
    padding-top: 60px
    text-align: center

  .address
    clear: both

    h2
      position: relative
      margin: 0
      padding: 0
      font-size: 1.25em
      color: $bg-grey
      text-align: left

      &:before
        content: ""
        position: absolute
        bottom: -15px
        left: 0
        width: 300px
        height: 1px
        background-color: $gs-yellow
    p
      padding: 2em 0 3em 0
      font-size: .75em
      line-height: 1.6em
      color: $bg-grey
      width: 415px

      a
        color: $gs-yellow
        font-size: 1em

    a
      font-size: .75em
      text-decoration: underline

  .bottom
    padding-top: 50px

  a
    padding-top: 3em
    font-size: .75em
    text-decoration: underline

.imprint

  &.header
    height: 706px
    width: 100%
    background: url('../images/other/header_impressum.jpg') no-repeat center

  &.legal
    width: 680px
    margin: 0 auto
    padding: 0 280px 0 0
    text-align: left

    h2
      margin: 2em 0
      font-size: 1.25em
      text-align: left
      padding: 0 1em

    p
      font-size: .75em
      color: $bg-grey
      padding: 0 1em

    .link-container
      float: left
      margin-top: 3em
      padding: 0 1em

.jobs

  &.header
    height: 706px
    width: 100%
    background: url('../images/other/header_jobs.jpg') no-repeat center

    .min-margin
      margin: 1
      padding: 0

    .min-margin-bottom
      margin-bottom: 1em
      padding-bottom: 0

  .job-card
    position: relative
    float: left
    width: 140px
    height: 140px
    padding: 10px
    border: 1px solid #bababa
    margin: 5px

    &:hover
      &:after
        content: "\e604"
        position: absolute
        top: .5em
        left: 4em
        color: $bg-grey
        font-size: 1.5em
        font-family: "IconFont"

    .job-title
      display: block
      margin: 2em auto 2em auto
      font-size: 1em
      color: $bg-grey
      font-family: "Novecentosanswide"
      font-weight: 600
      text-align: center
      line-height: 1.5em

    .job-short-desc
      display: block
      margin: 1em auto
      font-size: 1em
      color: $bg-grey
      text-align: center
      line-height: 1.6em

  &.emo
    padding-top: 60px

  .job-description
    clear: both
    width: 960px
    margin: 0
    padding: 0
    font-size: .75em

    p

    h2
      margin: 3em 0 0 0
      padding: 0
      font-size: 1.5em
      color: $bg-grey
      text-align: left

    h3
      position: relative
      margin: 1em 0 0 0
      padding: 0
      font-size: 1.25em
      color: $bg-grey
      text-align: left

    .job-requirements
      width: 910px
      padding: 20px
      border: 1px solid $bg-grey
      margin: 4em 0

    .button
      display: inline-block
      margin-top: 1.5em
      padding: 1em 0 0 0
      height: 30px
      color: $gs-yellow
      text-decoration: none
      text-align: center
      font-size: 1em

  .link-container
    margin-top: 5em

.presse
  text-align: left

  &.header
    height: 706px
    width: 100%
    background: url('../images/other/header_presse.jpg') no-repeat center

  .press-images
    width: 750px
    vertical-align: middle

    img
      display: inline-block
      margin: 2.6em

    .hat
      margin-bottom: 4em

  .button
    float: left

  ul
    clear: both
    float: left
    font-size: .75em
    padding: 1em

  .link-container
    clear: both
    margin-top: 5em !important
    float: left
