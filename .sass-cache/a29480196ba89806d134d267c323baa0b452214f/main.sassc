    clear: both

    h2
      position: relative
      margin: 0
      padding: 0
      font-size: 1.25em
      color: $bg-grey
      text-align: left

      &:before
        content: ""
        position: absolute
        bottom: -15px
        left: 0
        width: 300px
        height: 1px
        background-color: $gs-yellow
    p
      padding: 2em 0 3em 0
      font-size: .75em
      line-height: 1.6em
      color: $bg-grey
      width: 415px

      a
        color: $gs-yellow
        font-size: 1em

    a
      font-size: .75em
      text-decoration: underline

  .bottom
    padding-top: 50px

  a
    padding-top: 3em
    font-size: .75em
    text-decoration: underline

.imprint

  &.header
    height: 706px
    width: 100%
    background: url('../images/other/header_impressum.jpg') no-repeat center

  &.legal
    width: 680px
    margin: 0 auto
    padding: 0 280px 0 0
    text-align: left

    h2
      margin: 2em 0
      font-size: 1.25em
      text-align: left

    p
      font-size: .75em
      color: $bg-grey

    .link-container
      float: left
      margin-top: 3em

.jobs

  &.header
    height: 706px
    width: 100%
    background: url('../images/other/header_jobs.jpg') no-repeat center

    .min-margin
      margin: 1
      padding: 0

    .min-margin-bottom
      margin-bottom: 1em
      padding-bottom: 0

  .job-card
    position: relative
    float: left
    width: 140px
    height: 140px
    padding: 10px
    border: 1px solid #bababa
    margin: 5px

    &:hover
      &:after
        content: "\e604"
        position: absolute
        top: .5em
        left: 4em
        color: $bg-grey
        font-size: 1.5em
        font-family: "IconFont"

    .job-title
      display: block
      margin: 2em auto 2em auto
      font-size: 1em
      color: $bg-grey
      font-family: "Novecentosanswide"
      font-weight: 600
      text-align: center
      line-height: 1.5em

    .job-short-desc
      display: block
      margin: 1em auto
      font-size: 1em
      color: $bg-grey
      text-align: center
      line-height: 1.6em

  &.emo
    padding-top: 60px

  .job-description
    clear: both
    width: 960px
    margin: 0
    padding: 0
    font-size: .75em

    p

    h2
      margin: 3em 0 0 0
      padding: 0
      font-size: 1.5em
      color: $bg-grey
      text-align: left

    h3
      position: relative
      margin: 1em 0 0 0
      padding: 0
      font-size: 1.25em
      color: $bg-grey
      text-align: left

    .job-requirements
      width: 910px
      padding: 20px
      border: 1px solid $bg-grey
      margin: 4em 0

    .button
      display: inline-block
      margin-top: 1.5em
      padding: 1em 0 0 0
      height: 30px
      color: $gs-yellow
      text-decoration: none
      text-align: center
      font-size: 1em

  .link-container
    margin-top: 5em

.presse
  text-align: left

  &.header
    height: 706px
    width: 100%
    background: url('../images/other/header_presse.jpg') no-repeat center

  .press-images
    width: 750px
    vertical-align: middle

    img
      display: inline-block
      margin: 2.6em

    .hat
      margin-bottom: 4em

  .button
    float: left

  ul
    clear: both
    float: left
    font-size: .75em
    padding: 1em

  .link-container
    clear: both
    margin-top: 5em !important
    float: left
