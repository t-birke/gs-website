  height: 50px
  line-height: normal !important
  font-size: 12px
  font-family: 'Novecentosanswide'
  font-weight: 600
  color: $gs-yellow
  text-transform: uppercase
  outline: none
  border: 1px solid $gs-yellow
  background-color: transparent

  &:hover
