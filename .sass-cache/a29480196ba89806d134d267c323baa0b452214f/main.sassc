.reference-section
  position: relative
  background: url('../images/referenzen_header.jpg') no-repeat center -450px
  overflow: hidden
  z-index: -2

  .menu
    overflow: hidden
    margin-top: 112px
    height: 12em
    width: 100%
    background-color: $bg-grey
    text-align: center

    img
      display: inline-block
      margin: 2em

    .selectbox
      +border-radius(3px)
      +appearance(none)
      position: relative
      margin: 4em auto
      border: 1px solid $gs-yellow
      width: 9em
      padding-left: 1em
      overflow: hidden
      height: 2em
      line-height: 1.8em
      text-align: center
      font-size: 1.2em

      &:after
        content: "\25BC"
        position: absolute
        top: .4em
        right: 1.6em
        font-size: .4em
