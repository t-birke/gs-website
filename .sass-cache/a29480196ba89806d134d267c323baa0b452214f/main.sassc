    &.active
      color: $gs-yellow

    .ico
      display: inline-block
      font-size: 2.5em
      margin-bottom: .6em

      &.moveright
        margin-left: 20px

  .arrow
    position: absolute
    top: 13.5em
    left: 105px
    height: 0em
    width: 0em
    border-left: 6em solid transparent
    border-right: 6em solid transparent
    border-bottom: 1.5em solid white

.info-box
  float: left
  width: 18.75em
  height: 23.875em
